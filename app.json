{
  "expo": {
      "name": "Connect Pec Produtor",
      "slug": "connectpecprodutor",
      "owner": "agenciaboz-dev",
      "version": "1.0.4",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "light",
      "splash": {
          "image": "./assets/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
          "userInterfaceStyle": "light",
          "supportsTablet": false,
          "bundleIdentifier": "br.app.connectpecprodutor",
          "associatedDomains": ["applinks:connectpecprodutor.boz.app.br"]
      },
      "android": {
          "userInterfaceStyle": "light",
          "adaptiveIcon": {
              "foregroundImage": "./assets/foreground-icon.png",
              "monochromeImage": "./assets/adaptive-icon.png",
              "backgroundColor": "#ffffff"
          },
          "softwareKeyboardLayoutMode": "resize",
          "package": "com.agenciabozdev.connectpecprodutor",
          "permissions": ["android.permission.RECORD_AUDIO"]
      },
      "web": {
          "favicon": "./assets/favicon.png"
      },
      "notification": {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
      },
      "plugins": [
          ["./plugins/withDisableForcedDarkModeAndroid.js", {}],
          ["./plugins/withAndroidLargeHeap.js", {}],
          [
              "expo-image-picker",
              {
                  "photosPermission": "The app accesses your photos to let you upload it in your profile."
              }
          ],
          [
              "expo-font",
              {
                  "fonts": []
              }
          ],
          [
              "expo-notifications",
              {
                  "icon": "./assets/icon.png",
                  "color": "#ffffff"
              }
          ]
      ],
      "extra": {
          "eas": {
              "projectId": "8fb54094-69ac-4ea6-a512-d24c3825c481"
          }
      }
  }
}
